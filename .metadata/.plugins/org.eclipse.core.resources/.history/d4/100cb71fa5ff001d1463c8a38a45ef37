package com.patterns;

public class Pattern11 {

	public static void main(String[] args) {
	
		char c = 'e';
//		for (int k=0;k<=5; k++) {
//			
//		}
		String a=" ";
		for (int i = 5; i >= 1; i--) {
		    for (int j = i; j <=5 ; j++) {
		        System.out.print((char) (c - (5 - j))+" ");
		    }	
		    System.out.println();
		    
		}

	}
}


