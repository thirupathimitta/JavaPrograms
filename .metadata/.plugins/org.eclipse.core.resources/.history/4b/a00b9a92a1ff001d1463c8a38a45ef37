package com.patterns;

public class Pattern11 {

	public static void main(String[] args) {
	
		
		for (int i = 5; i >= 1; i--) {
			
			for(int k=1;k<=5; k++) {
				int a=1;
				for(int j=1;j<=5-i;j++) {
					System.out.print(" ");
				}
			}
		    for (int j = i; j <=5 ; j++) {
		    	
		        System.out.print((char) ('e' - (5 - j)));
		    }
		    System.out.println();
		}

	}
}


