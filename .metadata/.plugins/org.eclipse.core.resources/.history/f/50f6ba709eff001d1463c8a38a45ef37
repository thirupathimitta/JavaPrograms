package com.patterns;

public class Pattern11 {

	public static void main(String[] args) {
		

		for (int i = 5; i >= 1; i--) {
//			 for (int j=1;j<=5-i;j++) {
//			 System.out.print(" ");
//		 }
		    for (int j = i; j <= 5; j++) {
		    	
		        System.out.print((char) ('e' - (5 - j)));
		    }
		    System.out.println();
		}
//		 for(int i=1;i<=5;i++) {
//			 for (int j=1;j<=5-i;j++) {
//				 System.out.print(" ");
//			 }
//			 for(int k=6-i;k<=5;k++) {
//				 System.out.print(k+" ");
//			 }
//			 System.out.println();
//		 }



	}

}
