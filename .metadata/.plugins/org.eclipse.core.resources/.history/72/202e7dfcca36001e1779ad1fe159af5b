package com.generics;

class Box<T> {
    private T value;
    
    public void setValue(T value) {
        this.value = value;
    }
    
    public T getValue() {
        return value;
    }
}

public class GeneticsClass {
	

	
	    public static void main(String[] args) {
	        Box<Integer> intBox = new Box<>();
	        intBox.setValue(42);
	        int value = intBox.getValue();
	        System.out.println(value); // Outputs: 42
	    
	}


}
