package com.patterns;

public class Pattern11 {

	public static void main(String[] args) {
		int size = 5;

		int size = 5;

		for (int i = 0; i < size; i++) {
		    for (int j = 0; j < size - i - 1; j++) {
		        System.out.print("  ");
		    }
		    for (int k = i; k >= 0; k--) {
		        System.out.print((char) ('a' + k) + " ");
		    }
		    System.out.println();
		}

		/*
		 * char c = 'e'; // for (int k=0;k<=5; k++) { // // }
		 * 
		 * for (int i = 5; i >= 1; i--) { for (int j = i; j <=5 ; j++) {
		 * System.out.print((char) (c - (5 - j))+" "); } System.out.println();
		 * 
		 * }
		 */

	}
}


