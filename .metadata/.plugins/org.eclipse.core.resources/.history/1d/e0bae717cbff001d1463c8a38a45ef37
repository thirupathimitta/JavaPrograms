package com.patterns;

public class Pattern7 {

	public static void main(String[] args) {
		
		int n=5;
		for(int i=1;i<=n; i++) {
			
//			int a=1;
			
			for(int j=i;j<=n;j++) {
				System.out.print(" ");
			}
			
			for(int k=1;k<=i;k++) {
				
				System.out.print(k+" ");
//				a++;
			}
			System.out.println();
		}
	}

}
